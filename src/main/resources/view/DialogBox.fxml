<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>

<fx:root fx:id="dialogHolder" alignment="TOP_RIGHT" minHeight="-Infinity" minWidth="-Infinity" style="-fx-background-color: #EE8EC7; -fx-background-radius: 20; -fx-border-color: white; -fx-border-radius: 20;" type="javafx.scene.layout.HBox" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
  <children>
    <Label fx:id="dialog" alignment="CENTER" contentDisplay="CENTER" text="Label" textFill="WHITE" wrapText="true">
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding></Label>
    <ImageView fx:id="displayPicture" pickOnBounds="true" preserveRatio="true" />
  </children>
  <padding>
    <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
  </padding>
</fx:root>
